{
  "name": "rallyon",
  "version": "1.0.0",
  "description": "RallyOn developer tooling",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run format:check",
    "organizer:install": "npm install --prefix application/organizer",
    "organizer:dev": "npm start --prefix application/organizer",
    "organizer:lint": "npm run lint --prefix application/organizer",
    "organizer:test": "npm run test --prefix application/organizer",
    "organizer:test:ci": "npm run test:ci --prefix application/organizer",
    "organizer:test:e2e": "npm run test:e2e --prefix application/organizer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlambertz/rallyon.git"
  },
  "keywords": [],
